<% obj.each do |f| %>
  <tr>
      <td>Title: <%= f.title %></td>
      <td>Description <%= f.description %></td>
      <td><%= link_to  'Edit', edit_article_path(f) %></td>
      <td><%= link_to  'Show', article_path(f) %></td>
      <td><%= link_to  'Delete', article_path(f), method: :delete, data: {confirm: "Are you sure?"} %></td>
      <td><small>
          Created by: <%= f.user.username if f.user %>,
          <%= time_ago_in_words(f.created_at) %> ago,
          last updated: <%= time_ago_in_words(f.updated_at) %> ago
      </small>
    </td>
    </tr>
  <% end %>